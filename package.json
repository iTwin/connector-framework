{
  "name": "@itwin/connector-framework",
  "version": "1.0.0-dev.0",
  "description": "iTwin Connector Framework",
  "main": "lib/connector-framework.js",
  "typings": "lib/connector-framework",
  "license": "MIT",
  "engines": {
    "node": ">=12.22.0 < 14.0 || >=14.17.0 <17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/connector-framework"
  },
  "scripts": {
    "build": "git config --local core.hooksPath .githooks && tsc 1>&2 && npm run copy:assets && npm run copy:test-assets",
    "clean": "rimraf lib",
    "copy:assets": "cpx \"./src/assets/**/*\" ./lib/assets",
    "copy:config": "internal-tools copy-config",
    "copy:test-assets": "cpx \"./src/test/assets/**/*\" ./lib/test/assets",
    "lint": "eslint -f visualstudio \"./src/**/*.ts\" 1>&2",
    "pretest": "cpx ./src/test/logging.config.json ./lib/test",
    "test": "npm run test:standalone",
    "test:standalone": "npm run pretest && mocha './lib/test/standalone/**/*.js'",
    "test:integration": "npm run pretest && mocha --timeout 100000 './lib/test/integration/**/*.js'",
    "copy:itsyn-assets": "cpx \"./manifest.itsyn.yml\" \"lib/test/TestConnector/\" && cpx \"./node.exe\" \"lib/test/TestConnector/\"",
    "test:connector": "node lib/test/TestConnector/Main.js lib/test/assets/TestArgs.json"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "peerDependencies": {
    "@bentley/imodelhub-client": "3.0.0-extension.1",
    "@itwin/projects-client": "^3.0.0-extension.1",
    "@itwin/core-backend": "^3.0.0-extension.1",
    "@itwin/core-bentley": "^3.0.0-extension.1",
    "@itwin/core-common": "^3.0.0-extension.1",
    "@itwin/core-geometry": "^3.0.0-extension.1"
  },
  "devDependencies": {
    "@bentley/imodelhub-client": "3.0.0-extension.1",
    "@itwin/projects-client": "^0.3.0",
    "@itwin/build-tools": "^3.0.0-extension.1",
    "@itwin/core-backend": "^3.0.0-extension.1",
    "@itwin/core-bentley": "^3.0.0-extension.1",
    "@itwin/core-common": "^3.0.0-extension.1",
    "@itwin/core-electron": "^3.0.0-extension.1",
    "@itwin/core-geometry": "^3.0.0-extension.1",
    "@itwin/ecschema-metadata": "^3.0.0-extension.1",
    "@itwin/eslint-plugin": "^3.0.0-extension.1",
    "@itwin/oidc-signin-tool": "latest",
    "@itwin/presentation-common": "^3.0.0-extension.1",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "10.14.1",
    "@types/object-hash": "^1.3.0",
    "chai": "^4.1.2",
    "cpx2": "^3.0.0",
    "eslint": "^7.11.0",
    "dotenv": "10.0.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.4.0"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "object-hash": "^1.3.1"
  },
  "nyc": {
    "extends": "./node_modules/@itwin/build-tools/.nycrc"
  },
  "eslintConfig": {
    "plugins": [
      "@bentley"
    ],
    "extends": "plugin:@itwin/imodeljs-recommended"
  }
}
